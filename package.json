{
  "name": "chrolog",
  "version": "1.7.0",
  "description": "A automated time tracking tool",
  "main": "./out/main/index.js",
  "author": "lukylix",
  "homepage": "https://github.com/Lukylix/Chrolog",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "cross-env NODE_ENV=development electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": " npm run build && electron-builder --win --config",
    "build:mac": " npm run build && electron-builder --mac --config",
    "build:linux": " npm run build && electron-builder --linux --config",
    "react-devtools": "react-devtools"
  },
  "electronWebpack": {
    "skipPackage": true,
    "mainConfig": "./vite.config.js"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.1",
    "@electron/notarize": "^2.1.0",
    "@electron/remote": "^2.0.9",
    "@lwahonen/ffi-napi": "^4.0.12",
    "@lwahonen/ref-napi": "^4.0.8",
    "@preact/signals-react": "^1.3.6",
    "chrolog-iohook": "^1.3.7",
    "core-js": "^3.30.2",
    "cross-env": "^7.0.3",
    "electron-store": "^8.1.0",
    "events": "^3.3.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react-router-dom": "^6.11.1",
    "react-select": "^5.7.3",
    "recharts": "^2.6.2",
    "socket.io": "^4.6.2",
    "sqlite3": "^5.1.6",
    "sudo-prompt": "^9.2.1",
    "url": "^0.11.0",
    "util": "^0.12.5",
    "x11": "^2.3.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@svgr/rollup": "^8.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "27.0.3",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.21",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.2.1"
  },
  "packageManager": "yarn@4.0.1"
}
